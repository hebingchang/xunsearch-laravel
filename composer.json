{
    "name": "taxusorg/xunsearch-laravel",
    "description": "Laravel XunSearch engine.",
    "keywords": [
        "xunsearch", "XunSearch Engine", "Laravel Scout engine", "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taxusorg",
            "email": "taxus@taxus.org"
        }
    ],
    "require": {
        "php": ">=7.0",
        "Laravel/Scout": "^6.0",
        "hightman/xunsearch": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Taxusorg\\XunSearchLaravel\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taxusorg\\XunSearchLaravel\\XunSearchServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v2.1.x-dev"
        }
    }
}
